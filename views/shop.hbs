 <!-- Start Shop Page -->
 <div class="page-shop-sidebar left--sidebar summer">
	<h2 class="all_prod"> </h2>
	<div class="container">
		<div class="row">
			<div class="col-lg-2 col-12 order-2 order-lg-1 md-mt-40 sm-mt-40">
				<div class="shop__sidebar">
					<aside class="wedget__categories poroduct--cat">
					</aside>
				</div>
			</div>
			<div class="col-lg-9 col-12 order-1 order-lg-2  bg--white">
				<div class="row">
					<div class="col-lg-12" style="background-color: #D3F5FD">
						<div class="shop__list__wrapper d-flex flex-wrap flex-md-nowrap justify-content-between">
							<div class="shop__list nav justify-content-center" role="tablist">
								<a class="nav-item nav-link active" data-toggle="tab" href="#nav-grid" role="tab"><i class="fa fa-th"></i></a>
								<a class="nav-item nav-link" data-toggle="tab" href="#nav-list" role="tab"><i class="fa fa-list"></i></a>
							</div>
							<p style="margin-left: 10%;">Showing 1â€“11 of 11 results</p>
							<div class="orderby__wrapper">
									<span>Sort By</span>
									<select class="shot__byselect" id="sorted" name="sorted" onchange="window.location.href=this.value">
										<option value="{{value_d}}">{{title_d}}</option>
										<option value="{{value_p}}">{{title_p}}</option>
									</select>								
							</div>				
						</div>
					</div>
				</div>
				<div class="tab__container">
					<div class="shop-grid tab-pane fade show active" id="nav-grid" role="tabpanel">
						<div class="row" style="padding-top: 30px; justify-content: space-around; margin-bottom: 3em;">

			<!-- nav-grid -->
							<!-- Start Single Product -->
						{{#each item}}
							<div class="product product__style--3 col-lg-4 col-md-4 col-sm-6 col-12">
								<div class="product__thumb">
									<a class="first__img" href="item/{{id}}"><img src="/images/items/{{{image}}}"></a>
								</div>
								<div class="product__content content--center">
									<h4><a href="item">{{name}}</a></h4>
									<ul class="prize d-flex">
										<li>PHP{{price}}</li>
									</ul>
									<div class="action">
										<div class="actions_inner">
											<ul class="add_to_links">
												<li><a class="cart" href="cart"><i class="bi bi-shopping-bag4"></i></a></li>
												<li><a data-id="{{id}}" data-toggle="modal" title="Quick View" class="quickview modal-view detail-link" href="#productmodal{{id}}"><i class="bi bi-search"></i></a></li>
											</ul>
										</div>
									</div>
									<div class="product__hover--content">
										<ul class="rating d-flex">
											<li class="on"><i class="fas fa-star"></i></li>
											<li class="on"><i class="fas fa-star"></i></li>
											<li class="on"><i class="fas fa-star"></i></li>
											<li class="on"><i class="fas fa-star"></i></li>
											<li><i class="fa fa-star-o"></i></li>
										</ul>
									</div>
								</div>
							</div>
							<!-- End Single Product -->
						{{/each}}		
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<!-- End Shop Page -->
		<!-- QUICKVIEW PRODUCT -->
		{{#each item}}
		<div id="quickview-wrapper">
		    <!-- Modal -->
		    <div class="modal fade" id="productmodal{{id}}" tabindex="-1" role="dialog">
		        <div class="modal-dialog modal__container" role="document">
		            <div class="modal-content">
		                <div class="modal-header modal__header">
		                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
		                </div>
		                <div class="modal-body">
		                    <div class="modal-product">
		                        <!-- Start product images -->
		                        <div class="product-images">
		                            <div class="main-image images">
		                                <img alt="big images" src="/images/items/{{{image}}}">
		                            </div>
		                        </div>
		                        <!-- end product images -->
		                        <div class="product-info" style=" justify-content: space-between;">
									<h1>{{name}}</h1>
		                            <div class="rating__and__review">
										<div class="review">
		                                    <a href="#"> 4 customer reviews</a>
										</div>
										<ul class="rating d-flex">
											<li class="on"><i class="fas fa-star"></i></li>
											<li class="on"><i class="fas fa-star"></i></li>
											<li class="on"><i class="fas fa-star"></i></li>
											<li class="on"><i class="fas fa-star"></i></li>
											<li><i class="fa fa-star-o"></i></li>
										</ul>
		                            </div>
		                            <div class="price-box-3">
		                                <div class="s-price-box">
		                                    <span class="new-price">PHP{{price}}</span>
		                                </div>
		                            </div>
		                            <div class="quick-desc">
		                                {{description}}
		                            </div>
		                            <div class="addtocart-btn">
		                                <a href="addCart">Add to cart</a>
		                            </div>
		                        </div>
		                    </div>
		                </div>
		            </div>
		        </div>
		    </div>
		</div>
		{{/each}}
		<!-- END QUICKVIEW PRODUCT -->
	</div>

