<!DOCTYPE html>
<html>
<head>
<!-- Meta -->
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1">
    <link rel="shortcut icon" href="/images/fav.gif" type="image/x-icon">

  
  <title>Nookzada | Profile</title>
<!-- CSS -->
  <link rel="stylesheet" type="text/css" href="/web/assets/mobirise-icons2/mobirise2.css">
  <link rel="stylesheet" type="text/css" href="/tether/tether.min.css">
  <link rel="stylesheet" type="text/css" href="/bootstrap/css/bootstrap.min.css">
  <link rel="stylesheet" type="text/css" href="/bootstrap/css/bootstrap-grid.min.css">
  <link rel="stylesheet" type="text/css" href="/dropdown/css/style.css">
  <link rel="stylesheet" type="text/css" href="/socicon/css/styles.css">
  <link rel="stylesheet" type="text/css" href="/theme/css/style.css">
  <link rel="preload" as="style" href="/mobirise/css/mbr-additional.css">
  <link rel="stylesheet" href="/mobirise/css/mbr-additional.css" type="text/css">
  <!-- custom style sheet -->
	<link rel="stylesheet" type="text/css" href="css/bootstrap.min.css">
	<link rel="stylesheet" type="text/css" href="css/plugins.css">
	<link rel="stylesheet" type="text/css" href="css/shopstyle.css">
	<link rel="stylesheet" type="text/css" href="css/star_rating.css">
  <script src="https://kit.fontawesome.com/917f90ecf4.js" crossorigin="anonymous"></script>
  <!-- google fonts-->
  <link type="text/css" href="https://fonts.googleapis.com/css2?family=Barlow+Semi+Condensed:wght@600&family=Barlow:wght@400;600&display=swap" rel="stylesheet">
</head>

<body>
<!-- Navbar -->
<section class="menu menu2 cid-s9pQC51C9p" once="menu" id="menu2-14">
    
    <nav class="navbar navbar-dropdown navbar-fixed-top navbar-expand-lg">
        <div class="container-fluid">
            <div class="navbar-brand">
                <span class="navbar-logo">
                    <a href="index">
                        <img src="/images/fav.gif" style="height: 3rem;">
                    </a>
                </span>
                <span class="navbar-caption-wrap"><a class="navbar-caption text-warning text-primary display-7" href="index">Nookzada</a></span>
            </div>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                <div class="hamburger">
                    <span></span>
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav nav-dropdown" data-app-modern-menu="true">
                    <li class="nav-item"><a class="nav-link link text-warning text-primary display-4" href="shop">Shop</a></li>
                    <li class="nav-item"><a class="nav-link link text-warning text-primary display-4" href="about">About Us</a></li>
                    <li class="nav-item"><a class="nav-link link text-warning text-primary display-4" href="contact">Contact</a></li>
                    <li class="nav-item dropdown">
                    {{#if user}}
                        <a class="nav-link link text-warning dropdown-toggle display-4" href="#" data-toggle="dropdown-submenu" aria-expanded="false">{{user.username}}</a>
                    {{else}}
                        <a class="nav-link link text-warning dropdown-toggle display-4" href="#" data-toggle="dropdown-submenu" aria-expanded="false">User</a>
                    {{/if}}
                    <div class="dropdown-menu">
                    {{#if user}}
                      <a class="text-warning dropdown-item display-4" href="logout">Sign Out</a>
                      <a class="text-warning dropdown-item display-4" href="profile">View Profile</a>
                    {{else}}  
                      <a class="text-warning dropdown-item display-4" href="login">Sign In</a>
                    {{/if}}  
                    </div>
                  </li>
                </ul>
                <!-- cart -->
                <div class="navbar-buttons mbr-section-btn"><a class="btn btn-primary display-4" href="cart"><span class="mobi-mbri mobi-mbri-shopping-cart mbr-iconfont mbr-iconfont-btn"></span>
                        CART</a></div>
            </div>
        </div>
    </nav>
    <div class="social-links">
            <a href="mailto:nookzadashop@gmail.com"><i class="fas fa-envelope"></i></a>
            <a href="tel:+62896706255135"><i class="fas fa-phone-alt"></i></a>
            <a href="https://twitter.com/nutellacooki"><i class="fab fa-twitter"></i></a>
    </div>
</section>
 <!-- Start main Content -->
 <div class="maincontent profile pb--55" style="padding-top: 7%;">
	<div class="container">
		<div class="row bg--white" style="padding: 30px 0 40px 20px; border-radius: 7px; justify-content: space-around;">
			<div class="col-lg-9 col-12">
				<div class="wn__single__product">
					<div class="row">
						<div class="col-lg-6 col-12" style="margin-left: -12%;">
							<div class="wn__fotorama__wrapper">
								<div class="fotorama wn__fotorama__action" data-nav="thumbs">
									<a class="first__img" style="margin-left: -10%;"><img src="images/isabelle_hi.gif"></a>
								</div>
							</div>
						</div>
						<div class="col-lg-7 col-12" style="margin: 5% 0 0 2%;">
						<!-- name -->
							<div class="product__info__main" style="margin: 5% 0 0 10%;">
								<h2>Hi! {{username}}</h2>
						<!-- No. of reviews -->
                                <div class="price-box reviewNo" style="margin: 20px 0 0 0;">
                                    <a>You made </a>
									<a>{{numReview}}</a> 
									<a> reviews</a>
                                </div>
                        <!-- Change password question -->
                        <br><hr>
                        <div class="box-tocart d-flex" style="margin-top: 25%">
                            <span style="font-size: 15;">Change password?</span>
                         <!-- Change password button-->
                            <div class="addtocart__actions">
                                <a class="tocart yes" href="" style="background-color:transparent; color: #0499F2;">
                                    Yes!
                                </a>
                            </div>
                        </div>
						</div>
					</div>
				</div>
				<div class="product__info__detailed" style="margin: 3% 0 0 2%;">
					<div class="pro_details_nav nav justify-content-start" role="tablist">
						<a class="nav-item nav-link active" data-toggle="tab" href="#nav-details" role="tab">Details</a>
					</div>
					<div class="tab__container">
						<!-- Start Single Tab Content -->
						<div class="pro__tab_label tab-pane fade show active" id="nav-details" role="tabpanel">

					<!-- Review -->
						<!-- Start 1st Review -->
						
						<div id="nav-details" role="tabpanel">
							<div class="review__attribute" style="float: left; margin-right: 20px;">
								<h2>Review History</h2>
								<h4>{{r_itemname}}</h4>
								<div class="review__ratings__type d-flex">
									<div class="review-ratings" style="margin-left: -5%;">
										<div class="rating-summary d-flex">
											<ul class="rating d-flex">
												<li><i class="zmdi zmdi-star"></i></li>
												<li><i class="zmdi zmdi-star"></i></li>
												<li><i class="zmdi zmdi-star"></i></li>
												<li class="off"><i class="zmdi zmdi-star"></i></li>
												<li class="off"><i class="zmdi zmdi-star"></i></li>
											</ul>
										</div>

									<div class="review-content" style="margin: 0 -10% 0 27%;">
										<p>{{r_date}}</p>
									</div>
								</div>
							</div>
                        </div>
						
				</div>
			</div>
		</div>
	</div>
</div>
<!-- End main Content -->

<!-- JS -->
  <script src="/web/assets/jquery/jquery.min.js"></script>
  <script src="/popper/popper.min.js"></script>
  <script src="/tether/tether.min.js"></script>
  <script src="/bootstrap/js/bootstrap.min.js"></script>
  <script src="/smoothscroll/smooth-scroll.js"></script>
  <script src="/dropdown/js/nav-dropdown.js"></script>
  <script src="/dropdown/js/navbar-dropdown.js"></script>
  <script src="/touchswipe/jquery.touch-swipe.min.js"></script>
  <script src="/viewportchecker/jquery.viewportchecker.js"></script>
  <script src="/theme/js/script.js"></script>

<!-- Scroll up -->  
 <div id="scrollToTop" class="scrollToTop mbr-arrow-up"><a style="text-align: center;"><i class="mbr-arrow-up-icon mbr-arrow-up-icon-cm cm-icon cm-icon-smallarrow-up"></i></a></div>
    <input name="animation" type="hidden">
  </body>
</html>